<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Livestock">
    <children>
        <Pane prefHeight="600.0" prefWidth="295.0" style="-fx-background-color: rgb(0,0,0);">
            <children>
                <ImageView fitHeight="179.0" fitWidth="271.0" layoutX="47.0" layoutY="-10.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../Kumbaya-Logo.png" />
                    </image>
                </ImageView>
                <Button layoutY="185.0" mnemonicParsing="false" onAction="#btnHomeClick" prefHeight="55.0" prefWidth="295.0" text="Home">
                    <font>
                        <Font size="25.0" />
                    </font></Button>
                <Button fx:id="btnContacts" layoutY="290.0" mnemonicParsing="false" onAction="#btnContactsClick" prefHeight="55.0" prefWidth="295.0" text="Contacts">
                    <font>
                        <Font size="25.0" />
                    </font></Button>
                <Label alignment="CENTER" layoutY="394.0" mnemonicParsing="false" prefHeight="55.0" prefWidth="295.0" style="-fx-background-color: gray;" text="Livestock" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="25.0" />
                    </font></Label>
                <Button layoutY="485.0" mnemonicParsing="false" onAction="#btnMarketplaceClick" prefHeight="55.0" prefWidth="295.0" text="Marketplace">
                    <font>
                        <Font size="25.0" />
                    </font></Button>

                <Button fx:id="btnSignOff" layoutY="559.0" mnemonicParsing="false" onAction="#btnSignOffClick" prefHeight="40.0" prefWidth="83.0" text="Sign Off" />
            </children></Pane>
        <Label layoutX="485.0" layoutY="14.0" text="Livestock">
            <font>
                <Font name="System Bold" size="38.0" />
            </font></Label>
    </children>
</AnchorPane>
